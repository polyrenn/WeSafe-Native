{
  "expo": {
    "name": "WeSafe",
    "slug": "WeSafe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-iconx.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splashx.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "userInterfaceStyle": "light",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-iconx.png",
        "backgroundColor": "#ffffff"
      },
      
      "package": "com.polyrenn.WeSafe"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ad385eb0-c56e-43ca-b133-424a47593d0b"
      }
    },
    "plugins": ["@react-native-google-signin/google-signin", "@react-native-firebase/app", "@react-native-firebase/auth"]
  }
}
